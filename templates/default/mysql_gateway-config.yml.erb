---
cloud_controller_uri: <%= cloud_controller_url %>
service:
  name: mysql
  version: "1.8"
  description: 'MySQL RDBMS'
  plans: ['free']
  default_plan: 'free'
  tags: ['mysql', 'mysql-5.5']
  timeout: <%= node['cloudfoundry_mysql_service']['gateway']['timeout'] %>
  supported_versions: [<%= cfservicegw_supported_versions_string('mysql') %>]
  version_aliases:
  <% node['cloudfoundry_mysql_service']['gateway']['version_aliases'].each do |a, v| %>
    "<%= a %>": "<%= v %>"
  <% end %>

host: localhost
node_timeout: <%= node['cloudfoundry_mysql_service']['gateway']['node_timeout'] %>
index: 0
token: <%= node['cloudfoundry']['service_token'] %>
mbus: <%= cf_mbus_url %>
pid: <%= @pid_file %>
logging:
  level: <%= node['cloudfoundry_mysql_service']['gateway']['log_level'] %>
  file: <%=  @log_file %>
